    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2.3.4",
    "@vue/test-utils": "2.3.1",
    "eslint-plugin-vue": "8.7.1",
    "jsdom": "19.0.0",
    "standardx": "7.0.0",
    "vite": "2.9.15",
    "vitest": "0.34.6"
